<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffb5bfc4" android:endColor="#ffb5bfc4" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffb5bfc4" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80c07ab8" android:endColor="#a0c07ab8" android:angle="270.0" android:centerY="0.75" android:centerColor="#80c07ab8" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff1d99ff" android:endColor="#ff1d99ff" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff1d99ff" />
            </shape>
        </clip>
    </item>
</layer-list>