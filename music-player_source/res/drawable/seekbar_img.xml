<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ff151f29" android:endColor="#ff151f29" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff151f29" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ff5f737e" android:endColor="#ff5f737e" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff5f737e" />
            </shape>
        </clip>
    </item>
</layer-list>