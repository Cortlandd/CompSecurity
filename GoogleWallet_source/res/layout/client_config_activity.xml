<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:id="@id/ClientConfigLabel" android:background="@color/light" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/client_conf_debug_label" />
    <TextView android:id="@id/ClientConfigLastLoadTimeView" android:background="@color/light" android:layout_width="fill_parent" android:layout_height="wrap_content" />
    <ScrollView android:layout_width="fill_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="vertical" android:id="@id/FeatureFlagsLinearLayout" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="6.0dip" />
            <TextView android:id="@id/ClientConfigTextView" android:layout_width="fill_parent" android:layout_height="wrap_content" />
            <TextView android:id="@id/ClientConfigOverridesTextView" android:layout_width="fill_parent" android:layout_height="wrap_content" />
            <TextView android:id="@id/AppControlTextView" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="6.0dip" />
            <Button android:id="@id/GetGlobalResourcesButton" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/debug_sync_global_resources" />
            <Button android:id="@id/ClearOverriddenConfigButton" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/debug_clear_overridden_client_config" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>