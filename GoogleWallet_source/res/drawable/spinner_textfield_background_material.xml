<?xml version="1.0" encoding="utf-8"?>
<inset android:insetLeft="4.0dip" android:insetRight="4.0dip" android:insetBottom="4.0dip"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        <item android:state_checked="false" android:state_pressed="false">
            <layer-list>
                <item>
                    <nine-patch android:src="@drawable/abc_textfield_default_mtrl_alpha" android:tint="?colorControlNormal" />
                </item>
                <item>
                    <nine-patch android:src="@drawable/spinner_color_control_normal" />
                </item>
            </layer-list>
        </item>
        <item>
            <layer-list>
                <item>
                    <nine-patch android:src="@drawable/abc_textfield_activated_mtrl_alpha" android:tint="?colorControlActivated" />
                </item>
                <item>
                    <nine-patch android:src="@drawable/spinner_color_control_activated" />
                </item>
            </layer-list>
        </item>
    </selector>
</inset>