<layout name="hello_world" lang="en">
  <column-layout num-columns="1">
    <text row="0"
        column="0"
        padding-left="8pt"
        padding-top="10pt"
        padding-bottom="2pt"
        max-lines="1"
        font-family="sans-serif"
        font-size="14pt"><inline-link font-family="monospace" font-weight="bold" tap-uri="http://example.com">Hello World</inline-link>.</text>
    <text row="1"
        column="0">
      <date reference="relative">2013-09-02T05:00:00.000-08:00</date>
    </text>
    <spinner row="2"
        column="0"
        tap-uri="comgooglewallet:///objects/instance/1234/remote_action?name=refresh http://example.com"/>
  </column-layout>
</layout>
