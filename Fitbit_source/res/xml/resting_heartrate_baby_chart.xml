<?xml version="1.0" encoding="utf-8"?>
<ai:chart
  xmlns:ai="http://www.artfulbits.com/android/aiCharts">
    <ai:area background="@color/transparent" grid="@color/transparent">
        <area.xaxis grid_visible="false" labels_visible="false" padding="0dp" visible="false" />
        <area.yaxis grid_visible="true" labels_mode="MultiRows" labels_visible="true" padding="0dp" visible="true" />
    </ai:area>
    <ai:legend visible="false" />
    <ai:title visible="true" dock="Top" />
</ai:chart>