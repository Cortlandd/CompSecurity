<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/cpe_background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/cpe_progressbar_corner_radius" />
            <solid android:color="@color/cpe_strength_trackbar_background_color" />
        </shape>
    </item>
    <item android:id="@id/cpe_secondary_progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/cpe_progressbar_corner_radius" />
                <solid android:color="@color/cpe_strength_trackbar_background_color" />
            </shape>
        </clip>
    </item>
    <item android:id="@id/cpe_progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/cpe_progressbar_corner_radius" />
                <solid android:color="@color/cpe_strength_trackbar_foreground_color" />
            </shape>
        </clip>
    </item>
</layer-list>