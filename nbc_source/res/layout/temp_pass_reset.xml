<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Environment" />
    <RadioGroup android:orientation="vertical" android:id="@id/tpr_radio_group_environment_url" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <RadioButton android:id="@id/tpr_radio_prequal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:text="Prequal" />
        <RadioButton android:id="@id/tpr_radio_release" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Release" />
        <RadioButton android:id="@id/tpr_radio_custom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Custom" />
    </RadioGroup>
    <EditText android:id="@id/tpr_edit_custom_url" android:layout_width="fill_parent" android:layout_height="wrap_content" android:inputType="text" />
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Actions" />
    <LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent">
        <Button android:id="@id/tpr_btn_back_to_main" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Cancel" android:layout_weight="1.0" />
        <Button android:id="@id/tpr_btn_reset" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Reset" android:layout_weight="1.0" />
    </LinearLayout>
</LinearLayout>