<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ActionBarTitle" parent="@style/ActionBarTitleCommon">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/gray_back_button_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="CallEndButton">
        <item name="android:layout_width">@dimen/end_call_button_size</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/padding4</item>
    </style>
    <style name="ChatSuggestButton" parent="@style/ChatSuggestButtonBase">
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="IncomingCallButtonGap">
        <item name="android:visibility">gone</item>
    </style>
    <style name="LinkingErrorContainer">
        <item name="android:padding">@dimen/size_2x</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="LinkingErrorSubTitle" parent="@style/LinkingErrorContentBase">
        <item name="android:textSize">@dimen/font_small_medium</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="LinkingErrorTitle" parent="@style/LinkingErrorContentBase">
        <item name="android:textSize">@dimen/font_large</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/linking_error_padding</item>
        <item name="android:paddingBottom">@dimen/size_2x</item>
    </style>
    <style name="SignInHeaderVisibility">
        <item name="android:visibility">visible</item>
    </style>
    <style name="SigninContent" parent="@style/SigninContentBase" />
    <style name="SignoutLabelsContainer" parent="@style/SigninContent">
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="DialPadCountryButton">
        <item name="android:textColor">@color/skype_dark_grey</item>
        <item name="android:background">@drawable/focus_highlight</item>
        <item name="android:paddingTop">2.0dip</item>
        <item name="android:paddingBottom">2.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableRight">@drawable/small_arrow_right_selector</item>
    </style>
    <style name="DialpadContact">
        <item name="android:textSize">@dimen/font_small_medium</item>
    </style>
    <style name="LinkingCloudsMessageText">
        <item name="android:textSize">@dimen/font_large</item>
        <item name="android:textColor">@color/skype_blue</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">30.0dip</item>
        <item name="android:paddingRight">30.0dip</item>
    </style>
    <style name="LinkingTitleTextInverse" parent="@style/LinkingTitleText">
        <item name="android:textColor">@color/main_light_color</item>
        <item name="android:paddingTop">@dimen/size_2x</item>
    </style>
    <style name="SignInLogoSize">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>
