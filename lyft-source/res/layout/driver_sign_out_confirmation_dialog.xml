<?xml version="1.0" encoding="utf-8"?>
<me.lyft.android.ui.driver.DriverSignOutConfirmationDialogView style="@style/DialogContainerView"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout style="@style/DialogView">
        <TextView android:textStyle="bold" android:gravity="center" android:id="@id/title_text_view" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_margin="@dimen/span20" android:text="@string/driver_sign_out_confirmation_title" style="@style/PrimaryText" />
        <TextView android:gravity="center" android:id="@id/message_text_view" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/span20" android:layout_marginRight="@dimen/span20" android:layout_marginBottom="@dimen/span20" android:text="@string/driver_sign_out_confirmation_message" style="@style/SecondaryText" />
