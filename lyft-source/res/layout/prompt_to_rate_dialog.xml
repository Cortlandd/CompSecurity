<?xml version="1.0" encoding="utf-8"?>
<me.lyft.android.ui.passenger.v2.PromptToRateDialogView style="@style/DialogContainerView"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <FrameLayout style="@style/DialogWithCloseButtonView">
        <LinearLayout android:gravity="center_horizontal" android:layout_gravity="center_vertical" android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/DialogContainer">
            <TextView android:textSize="22.0sp" android:id="@id/dialog_title_text_view" android:text="@string/rate_app_dialog_title" style="@style/DialogTitle" />
            <TextView android:id="@id/message_txt" android:visibility="gone" android:text="@string/rate_app_dialog_message" style="@style/DialogMessage" />
            <View android:layout_marginTop="@dimen/span16" style="@style/HorizontalDialogDivider" />
            <Button android:id="@id/rate_app_positive_button" android:text="@string/rate_app_positive_button" style="@style/DialogButton" />
            <View style="@style/HorizontalDialogDivider" />
            <Button android:id="@id/rate_app_neutral_button" android:text="@string/rate_app_neutral_button" style="@style/DialogButton" />
            <View style="@style/HorizontalDialogDivider" />
            <Button android:id="@id/rate_app_negative_button" android:text="@string/rate_app_negative_button" style="@style/DialogButton" />
        </LinearLayout>
    </FrameLayout>
</me.lyft.android.ui.passenger.v2.PromptToRateDialogView>