<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ff535353" android:endColor="#ffffffff" android:angle="270.0" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="#ff535353" android:endColor="#ff262626" android:angle="270.0" />
                <corners android:radius="3.0dip" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <gradient android:startColor="#ffb9090b" android:endColor="#ff70060a" android:angle="270.0" />
                <corners android:radius="3.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>