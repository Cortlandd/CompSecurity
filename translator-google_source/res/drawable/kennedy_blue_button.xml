<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <corners android:radius="2.0px" />
            <stroke android:width="1.0px" android:color="#ff3079ed" />
            <gradient android:startColor="#ff4d90fe" android:endColor="#ff4787ed" android:angle="270.0" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="2.0px" />
            <stroke android:width="1.0px" android:color="#ff2f5bb7" />
            <gradient android:startColor="#ff4d90fe" android:endColor="#ff357ae8" android:angle="270.0" />
        </shape>
    </item>
</selector>