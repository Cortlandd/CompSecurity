<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10.0dip" />
            <gradient android:startColor="#ffe9e9e9" android:endColor="#ffe9e9e9" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffe9e9e9" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10.0dip" />
                <gradient android:startColor="#ff3f87ff" android:endColor="#ff3f87ff" android:centerColor="#ff3f87ff" />
            </shape>
        </clip>
    </item>
</layer-list>