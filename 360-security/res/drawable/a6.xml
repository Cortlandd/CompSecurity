<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="8.0dip" />
            <gradient android:startColor="#ffdddddd" android:centerY="1.0" android:centerColor="#ffdddddd" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="8.0dip" />
                <gradient android:startColor="#ff2196f3" android:endColor="#ff2196f3" android:centerY="1.0" />
            </shape>
        </clip>
    </item>
</layer-list>